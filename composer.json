{
    "name": "orthoplex/laravel11-challenge",
    "type": "project",
    "description": "Back-end (Laravel 11 + MySQL) scaffold for Orthoplex Solutions challenge",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0",
        "laravel/sanctum": "^4.0",
        "laravel/octane": "^2.7",
        "laravel/scout": "^10.0",
        "bacon/bacon-qr-code": "^2.0",
        "guzzlehttp/guzzle": "^7.9",
        "brick/math": "^0.12",
        "meilisearch/meilisearch-php": "^1.7",
        "darkaonline/l5-swagger": "^9.0",
        "laravel/tinker": "^2.9",
        "pragmarx/google2fa": "^9.0",
        "spatie/laravel-permission": "^6.21",
        "tymon/jwt-auth": "^2.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "orchestra/testbench": "^9.15",
        "phpunit/phpunit": "^10.5",
        "spatie/laravel-ignition": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Domain\\": "domain/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
